<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yuvaa.e_krishi">

    <uses-permission android:name="android.permission.INTERNET" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login" />
        <activity android:name=".about" />
        <activity android:name=".addproduct" />
        <activity android:name=".search" />
        <activity android:name=".cropview" />
        <activity
            android:name=".forgot"
            android:label="@string/title_activity_forgot" />
        <activity android:name=".thnku" />
        <activity
            android:name=".popup"
            android:label="Best Deal"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity android:name=".FarmerName"></activity>
    </application>

</manifest>